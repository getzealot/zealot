ruby:
  @schemes = @channel.scheme

.col-sm-12
  .nav-tabs-custom
    ul.nav.nav-tabs
      == render 'schemes/channels/list'
      / li.pull-right.header
      /   i.fa.fa-adn
      /   = @title
    .card
      .card-header
        h3.card-title
          = app_icon(@channel.latest_release, size: :medium, class: 'img-circle')
          .app-name
            = @title

